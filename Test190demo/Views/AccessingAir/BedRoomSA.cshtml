@model Test190demo.Models.BedSA
@{
    ViewBag.Title = "BedRoomSA";
    Layout = null;
}

<!DOCTYPE html>
<html data-wf-page="5ca8bd7071815dc15c45ba69" data-wf-site="5ca8715a71815d237244feff">
<head>
    <meta charset="utf-8">
    <title>Bed room inside</title>
    <link href="~/css/normalize.css" rel="stylesheet" type="text/css">
    <link href="~/css/webflow.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/evans-exceptional-project.webflow.css" rel="stylesheet" type="text/css">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">WebFont.load({ google: { families: ["Montserrat:100,100italic,200,200italic,300,300italic,400,400italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic", "Vollkorn:400,400italic,700,700italic", "Source Sans Pro:200,200italic,300,300italic,regular,italic,600,600italic,700,700italic,900,900italic"] } });</script>
    <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
    <script type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
</head>
<body class="body-7">
    <div data-collapse="medium" data-animation="over-right" data-duration="600" data-doc-height="1" class="navbar w-nav">
        <div class="nav-container w-container">
            <nav role="navigation" class="nav-menu w-nav-menu"><img src="~/images/logo-about.jpeg" width="100" alt="" class="mobile-logo w-hidden-main"><a href="~/Homepage/Index" class="w-inline-block"><img src="~/images/logotest11.jpeg" width="120" height="40" alt="" class="brand-image"></a><a href="~/Homepage/Index" class="nav-link w-nav-link">Home</a><a href="~/Homepage/about" class="nav-link w-nav-link">About</a><a href="~/AccessingAir/Index" class="nav-link w-nav-link">Assessing YOUR AIR</a><a href="~/Emissions/SearchSub" class="nav-link w-nav-link">Pollutants Around You</a></nav>
            <div class="menu-button w-nav-button">
                <div class="nav-icon w-icon-nav-menu"></div>
            </div>
        </div>
    </div>
    <header id="head" data-ix="page-load-fade-in" class="header">
        <div class="slide-2 w-slide">
            <div class="slide-container">
                <h1 class="heading-7">We will help you to <br>improve the indoor <br>air quality</h1>
                <h4 class="heading-8">Let us help you realize your air condition</h4>
            </div>
        </div>
    </header>
    <h1 class="heading-5" style="color:#333">Assessing your Bedroom &#x27;s air</h1>
    <br />
    <div class="w-form" style="color:#333">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

        <div class="form-horizontal col-md-8 col-md-offset-1">
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <h3 style="color:#333">Do you open your window in your bedroom?</h3>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EnumDropDownListFor(model => model.OpenWindow, null, htmlAttributes: new { @class = "form-control", @onchange = "changeStatus()" })
                        @Html.ValidationMessageFor(model => model.OpenWindow, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>

            <hr />
            <h3 style="color:#333">Do you have mould developing in your room?</h3>
            <h5 style="color:#333; left: 100px">Mould may grow indoors in wet or moist areas lacking adequate ventilation, including walls/ wallpaper, </h5>
            <h5 style="color:#333; left: 100px">ceilings, bathroom tiles, carpets (especially those with jute backing), insulation material and wood.</h5>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EnumDropDownListFor(model => model.Mould, null, htmlAttributes: new { @class = "form-control", @onchange = "changeStatus()" })
                        @Html.ValidationMessageFor(model => model.Mould, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>

            <hr />
            <h3 style="color:#333">Do you have plants in your room?</h3>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EnumDropDownListFor(model => model.Plants, null, htmlAttributes: new { @class = "form-control", @onchange = "changeStatus()" })
                        @Html.ValidationMessageFor(model => model.Plants, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>

            <hr />
            <h3 style="color:#333">Does anybody smoke in the house?</h3>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EnumDropDownListFor(model => model.Smoke, null, htmlAttributes: new { @class = "form-control", @onchange = "changeStatus()" })
                        @Html.ValidationMessageFor(model => model.Smoke, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>

            <hr />
            <h3 style="color:#333">Do you have a Carpet/Rug in your room?</h3>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EnumDropDownListFor(model => model.Carpet, null, htmlAttributes: new { @class = "form-control", @onchange = "changeStatus()" })
                        @Html.ValidationMessageFor(model => model.Carpet, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-2" style="left:50px">
                    <input type="submit" value="Create" class="button-5 w-button" />
                </div>
                <div class="form-group">
                    <div class="col-md-2" style="left:50px; top:5px">
                        <a href="~/AccessingAir/Index" class="button-6 w-button" style="margin-right:20px">Return</a>
                    </div>
                </div>
            </div>

        </div>
        }
    </div>
    <script src="~/js/CarSA.js"></script>
    <script src="https://d1tdp7z6w94jbb.cloudfront.net/js/jquery-3.3.1.min.js" type="text/javascript" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="~/js/webflow.js" type="text/javascript"></script>

    <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>
</html>

